<view class="cml-radio  cml-base cml-view cml-1fd7ff3c" style="{{computedCstyle}}">
  <view class="cml-radio-wrap  cml-base cml-view cml-1fd7ff3c" onTap="_cmlEventProxy" data-eventtap="changeSelect">
    <view class="cml-radio-select  cml-base cml-view cml-1fd7ff3c" style="{{radioStyle}}">
      <view class="{{ radioClass }}  cml-base cml-view cml-1fd7ff3c" style="{{radioInnerStyle}}">
      </view>
    </view>
    <text class="cml-radio-label  cml-base cml-text cml-1fd7ff3c">{{label}}</text>
  </view>
</view>