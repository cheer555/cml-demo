<view class="cml-checkbox  cml-base cml-view cml-5ebca3b8">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-5ebca3b8" onTap="_cmlEventProxy" data-eventtap="changeCheck">
    <view class="cml-checkbox-check  cml-base cml-view cml-5ebca3b8" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-5ebca3b8">
        <image class="inner-icon  cml-base cml-image cml-5ebca3b8" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-5ebca3b8">{{label}}</text>
  </view>
</view>