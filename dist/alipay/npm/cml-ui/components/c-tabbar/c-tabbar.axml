<view class="tab-bar-root {{layoutClass}}  cml-base cml-view cml-daccec60">
  <view class="tab-bar-wrap   cml-base cml-view cml-daccec60" style="{{tabbar.tabbarStyle || ';'}}">
    <view a:for="{{tabbar.list }}" a:key="text" class="tab-item-wrap  cml-base cml-view cml-daccec60">
      <view class="tab-content  cml-base cml-view cml-daccec60" onTap="_cmlInlineStatementEventProxy" data-arg0="{{index}}" data-args="index" data-eventtap="onTabbarItemTap">
        <view class="tab-icon-wrap  cml-base cml-view cml-daccec60" a:if="{{!top && hasIcon}}">
          <image a:if="{{!(index === tapedIndex)}}" class="tab-icon  cml-base cml-image cml-daccec60" src="{{item.icon}}" style="{{!(index === tapedIndex) ? item.iconStyle || '' : ';'}}"></image>
          <image a:if="{{index === tapedIndex}}" class="tab-icon  cml-base cml-image cml-daccec60" src="{{item.selectedIcon || item.icon}}" style="{{index === tapedIndex ? item.selectedIconStyle || '' : ''}}"></image>
        </view>
        <view class="tab-title  cml-base cml-view cml-daccec60">
          <text class="tab-text  cml-base cml-text cml-daccec60" style="{{index === tapedIndex ? tabbar.selectedTextStyle || 'color:#61c7fc' : tabbar.textStyle || 'color:#000000'}}">{{item.text}}</text>
        </view>
      </view>
    </view>
  </view>
  <view a:if="{{top}}" class="line-wrap  cml-base cml-view cml-daccec60" style="{{tabLineStyle}}"></view>

</view>