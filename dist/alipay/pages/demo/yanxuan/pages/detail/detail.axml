<view class=" cml-base cml-view cml-a7569ec4"><cml-buildin-page title="详情页" class=" cml-base cml-page cml-a7569ec4">
  <view class="content  cml-base cml-view cml-a7569ec4">
    <view class=" cml-base cml-view cml-a7569ec4"><myscroller class=" cml-base cml-myscroller cml-a7569ec4">
      <view class=" cml-base cml-view cml-a7569ec4">
        <view class="carousel-container  cml-base cml-view cml-a7569ec4"><swiper class="carousel-container  cml-base cml-carousel cml-a7569ec4" indicator-dots="{{true}}" current="{{1}}" circular="{{true}}">
          <swiper-item class="carousel-carousel-item  cml-base cml-carousel-item cml-a7569ec4" a:for="{{bannerImg}}" a:for-index="i" a:for-item="item" a:key="i" onTap="_cmlEventProxy" data-eventtap="change" data-idx="{{i}}">
            <image src="{{item.imgUrl}}" class="Img  cml-base cml-image cml-a7569ec4" />
          </swiper-item>
        </swiper></view>
      </view>
      <view class="description  cml-base cml-view cml-a7569ec4">
        <view class="description-item  cml-base cml-view cml-a7569ec4" a:for="{{descriptionInfo}}" a:for-index="i" a:for-item="item" data-idx="{{i}}">
          <view class="description-item-img  cml-base cml-view cml-a7569ec4">
            <image src="{{item.imgUrl}}" class="description-item-img-con  cml-base cml-image cml-a7569ec4" />
          </view>
          <view class="description-item-des  cml-base cml-view cml-a7569ec4">
            <text class="description-item-des-tag  cml-base cml-text cml-a7569ec4">{{item.tag1}}</text>
            <text class="description-item-des-tag  cml-base cml-text cml-a7569ec4">{{item.tag2}}</text>
          </view>
        </view>
      </view>
      <view class="section  cml-base cml-view cml-a7569ec4">
        <view class="section-money  cml-base cml-view cml-a7569ec4">
          <text class="class-money  cml-base cml-text cml-a7569ec4">{{money}}</text>
        </view>
        <view class="section-tag  cml-base cml-view cml-a7569ec4" onTap="_cmlEventProxy" data-eventtap="goto">
          <text class="section-tag-con  cml-base cml-text cml-a7569ec4">{{appTag}}</text>
        </view>
        <view class="section-evaluate  cml-base cml-view cml-a7569ec4">
          <view class="section-evaluate-info  cml-base cml-view cml-a7569ec4">
            <text class="section-evaluate-info-name  cml-base cml-text cml-a7569ec4">60s锦眠贡缎四件套</text>
            <text class="section-evaluate-info-desc  cml-base cml-text cml-a7569ec4">400根纯棉贡缎，入门奢享高阶柔滑</text>
          </view>
          <view class="section-evaluate-number  cml-base cml-view cml-a7569ec4">
            <text class="section-evaluate-number-total  cml-base cml-text cml-a7569ec4">26</text>
            <text class="section-evaluate-number-ping  cml-base cml-text cml-a7569ec4">用户评价</text>
          </view>
        </view>
      </view>
      <view class="coupon  cml-base cml-view cml-a7569ec4">
        <text class="coupon-tag  cml-base cml-text cml-a7569ec4">年货及时津贴</text>
        <text class="coupon-money  cml-base cml-text cml-a7569ec4">领津贴每满300减15</text>
        <text class="coupon-get  cml-base cml-text cml-a7569ec4">去领取</text>
      </view>
      <view class="map  cml-base cml-view cml-a7569ec4">
        <view class=" cml-base cml-view cml-a7569ec4"><map class=" cml-base cml-map cml-a7569ec4"></map></view>
      </view>
    </myscroller></view>
  </view>
  </cml-buildin-page></view>